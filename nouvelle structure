enum BeeType {
    QUEEN, GUARDIAN, DRONE
};

struct Bee ListBee;

typedef struct Bee {
    enum BeeType type;
    int iLifeTime;
    int iIdOfBee;
    int iNbQueens;
    Bee* nextBee;
}Bee;

void test ()
{
    Bee* bee = malloc(sizeof(Bee*));

    switch (bee->type)
    {
    case QUEEN:{
        break;
    }
    case DRONE: {
        break;
    }
    case GUARDIAN: {
        break;
    }
    default:
        break;
    }
}
